{
    "name": "wbrowar/guide",
    "description": "A CMS Guide for Craft CMS.",
    "type": "craft-plugin",
    "version": "4.0.0-beta.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "guide"
    ],
    "support": {
        "docs": "https://github.com/wbrowar/craft-guide/blob/master/README.md",
        "issues": "https://github.com/wbrowar/craft-guide/issues"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Will Browar",
            "homepage": "https://wbrowar.com"
        }
    ],
    "require": {
        "craftcms/cms": "^4.0.0-beta.1"
    },
    "autoload": {
        "psr-4": {
          "wbrowar\\guide\\": "src/"
        }
    },
    "extra": {
        "name": "Guide",
        "handle": "guide",
        "hasCpSettings": true,
        "hasCpSection": true,
        "changelogUrl": "https://raw.githubusercontent.com/wbrowar/craft-guide/master/CHANGELOG.md",
        "class": "wbrowar\\guide\\Guide"
    }
}
